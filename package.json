{
  "name": "learning-vscode-extension",
  "displayName": "Learning_VSCode_Extension",
  "description": "just learning vscode extension maker",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/fearless23/VSCode-Extension-learn"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:listLambdas"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "happyLambda",
          "title": "HL",
          "icon": "resources/dark/happyLambda.svg"
        }
      ]
    },
    "views": {
      "happyLambda": [
        {
          "id": "listLambdas",
          "name": "AWS Lambda Functions"
        },
        {
          "id": "listLayers",
          "name": "AWS Layers"
        }
      ]
    },
    "commands": [
      {
        "command": "happyLambda.refreshLambdas",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "happyLambda.viewLambda",
        "title": "View",
        "icon": {
          "light": "resources/light/lambda.svg",
          "dark": "resources/dark/lambda.svg"
        }
      },
      {
        "command": "happyLambda.refreshLayers",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "happyLambda.viewLayer",
        "title": "View",
        "icon": {
          "light": "resources/light/lambda.svg",
          "dark": "resources/dark/lambda.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "happyLambda.refreshLambdas"
        },
        {
          "command": "happyLambda.viewLambda"
        }
      ],
      "view/title": [
        {
          "command": "happyLambda.refreshLambdas",
          "when": "viewItem == lambdaFn ",
          "group": "navigation"
        },
        {
          "command": "happyLambda.refreshLayers",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "happyLambda.viewLambda",
          "when": "view == happyLambda & viewItem == lambdaFn",
          "group": "inline"
        },
        {
          "command": "happyLambda.viewLayer",
          "when": "view == happyLambda & viewItem == layer",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.596.0"
  },
  "publisher": "fearless"
}
